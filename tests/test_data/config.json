        "haplotypecaller": {
            "default": "haplotypecaller.vcf.gz",
            "mutation": "germline",
            "merged": "haplotypecaller.vcf.gz",
            "type": "SNV"
        },
        "vardict": {
            "default": "vardict.vcf.gz",
            "mutation": "somatic",
            "merged": "vardict.vcf.gz",
            "type": "SNV"
        }
