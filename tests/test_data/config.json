        "AATGATACGGCGACCACCGAGATCTACACTCTTTCCCTACACGACGCTCTTCCGATCT",
        "min_seq_length": "25"
    },
    "vcf": {
        "manta": {
            "default": [
                "diploidSV.vcf.gz", "somaticSV.vcf.gz",
                "candidateSV.vcf.gz", "candidateSmallIndels.vcf.gz"
            ],
            "merged": "manta.vcf.gz",
            "mutation": "somatic",
            "type": "SV"
        },
        "manta_germline": {
            "default": [
                "diploidSV.vcf.gz", "candidateSV.vcf.gz",
                "candidateSmallIndels.vcf.gz"
